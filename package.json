{
  "name": "swagger-http-router",
  "version": "1.5.0",
  "description": "A HTTP router based on your Swagger/OpenAPI definition.",
  "keywords": [
    "Swagger",
    "OpenAPI",
    "HTTP",
    "REST",
    "API"
  ],
  "main": "src/index.js",
  "metapak": {
    "configs": [
      "jsdocs",
      "jsarch",
      "readme",
      "travis",
      "eslint",
      "codeclimate",
      "jest",
      "babel"
    ],
    "data": {
      "files": "src/*.js tests/*.js",
      "testsFiles": "src/*.mocha.js tests/*.mocha.js"
    }
  },
  "scripts": {
    "architecture": "jsarch src/*.js tests/*.js > ARCHITECTURE.md",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "cli": "env NODE_ENV=${NODE_ENV:-cli}",
    "compile": "babel src --out-dir=dist",
    "cover": "jest --config=.jest.config.js --coverage",
    "coveralls": "npm run cover && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "cz": "env NODE_ENV=${NODE_ENV:-cli} git cz",
    "doc": "echo \"# API\" > API.md; jsdoc2md src/*.js tests/*.js >> API.md",
    "lint": "eslint src/*.js tests/*.js",
    "metapak": "metapak",
    "mocha": "mocha src/*.mocha.js tests/*.mocha.js",
    "prettier": "prettier --write src/*.js tests/*.js",
    "preversion": "npm t && npm run lint && npm run metapak -s && npm run compile",
    "test": "jest --config=.jest.config.js",
    "version": "npm run changelog && git add CHANGELOG.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nfroidure/swagger-http-router.git"
  },
  "author": "Nicolas Froidure",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nfroidure/swagger-http-router/issues"
  },
  "homepage": "https://github.com/nfroidure/swagger-http-router#readme",
  "dependencies": {
    "ajv": "^5.2.2",
    "body": "^5.1.0",
    "bytes": "^2.5.0",
    "camel-case": "^3.0.0",
    "common-services": "^3.0.0",
    "content-type": "^1.0.2",
    "debug": "2.6.1",
    "first-chunk-stream": "^2.0.0",
    "knifecycle": "^2.4.1",
    "negotiator": "^0.6.1",
    "qs": "^6.5.1",
    "siso": "^2.1.3",
    "statuses": "^1.3.1",
    "strict-qs": "^3.0.1",
    "swagger-parser": "^3.4.1",
    "yerror": "^2.1.0",
    "yhttperror": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "commitizen": "^2.9.6",
    "conventional-changelog-cli": "^1.3.8",
    "coveralls": "^3.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.19.0",
    "eslint-plugin-prettier": "^2.6.0",
    "istanbul": "^1.0.0-alpha.2",
    "jest": "^22.4.2",
    "jsarch": "^1.3.0",
    "jsdoc-to-markdown": "^3.1.0-0",
    "metapak": "^1.0.3",
    "metapak-nfroidure": "^4.1.0",
    "prettier": "^1.11.1",
    "sinon": "^2.1.0",
    "streamtest": "^1.2.2",
    "supertest": "^3.0.0"
  },
  "engines": {
    "node": ">=6.9.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "greenkeeper": {
    "ignore": [
      "commitizen",
      "cz-conventional-changelog",
      "conventional-changelog-cli",
      "jsdoc-to-markdown",
      "jsarch",
      "eslint",
      "eslint-config-prettier",
      "prettier",
      "jest",
      "coveralls",
      "istanbul"
    ]
  }
}
