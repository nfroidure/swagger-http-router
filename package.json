{
  "name": "swagger-http-router",
  "version": "2.0.0",
  "description": "A HTTP router based on your Swagger/OpenAPI definition.",
  "keywords": [
    "Swagger",
    "OpenAPI",
    "HTTP",
    "REST",
    "API"
  ],
  "main": "dist/index.js",
  "metapak": {
    "configs": [
      "main",
      "jsarch",
      "readme",
      "travis",
      "eslint",
      "codeclimate",
      "jest",
      "babel",
      "jsdocs"
    ],
    "data": {
      "files": "src/*.js tests/*.js",
      "testsFiles": "src/*.mocha.js tests/*.mocha.js",
      "ignore": [
        "dist"
      ]
    }
  },
  "scripts": {
    "architecture": "jsarch src/*.js tests/*.js > ARCHITECTURE.md",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "cli": "env NODE_ENV=${NODE_ENV:-cli}",
    "compile": "babel src --out-dir=dist",
    "cover": "npm run jest -- --coverage",
    "coveralls": "npm run cover && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "cz": "env NODE_ENV=${NODE_ENV:-cli} git cz",
    "doc": "echo \"# API\" > API.md; jsdoc2md src/*.js tests/*.js >> API.md",
    "jest": "NODE_ENV=test jest",
    "lint": "eslint src/*.js tests/*.js",
    "metapak": "metapak",
    "mocha": "mocha src/*.mocha.js tests/*.mocha.js",
    "precz": "npm t && npm run lint && npm run metapak -- -s",
    "prettier": "prettier --write src/*.js tests/*.js",
    "preversion": "npm t && npm run lint && npm run compile && npm run metapak -- -s",
    "test": "npm run jest",
    "version": "npm run changelog && git add CHANGELOG.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nfroidure/swagger-http-router.git"
  },
  "author": {
    "name": "Nicolas Froidure",
    "email": "nicolas.froidure@insertafter.com",
    "url": "http://insertafter.com/en/index.html"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nfroidure/swagger-http-router/issues"
  },
  "homepage": "https://github.com/nfroidure/swagger-http-router#readme",
  "dependencies": {
    "ajv": "^6.5.4",
    "body": "^5.1.0",
    "bytes": "^3.0.0",
    "camel-case": "^3.0.0",
    "common-services": "^3.2.0",
    "content-type": "^1.0.4",
    "debug": "4.1.0",
    "first-chunk-stream": "^2.0.0",
    "knifecycle": "^3.1.0",
    "negotiator": "^0.6.1",
    "qs": "^6.5.2",
    "siso": "^2.2.1",
    "statuses": "^1.5.0",
    "strict-qs": "^3.0.2",
    "swagger-parser": "^6.0.1",
    "yerror": "^2.1.3",
    "yhttperror": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "commitizen": "^3.0.4",
    "conventional-changelog-cli": "^2.0.5",
    "coveralls": "^3.0.2",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.7.0",
    "eslint-plugin-prettier": "^3.0.0",
    "jest": "^23.6.0",
    "jsarch": "^1.3.0",
    "jsdoc-to-markdown": "^4.0.1",
    "metapak": "^2.0.0",
    "metapak-nfroidure": "8.0.0",
    "prettier": "^1.14.3",
    "sinon": "^7.0.0",
    "streamtest": "^1.2.4",
    "supertest": "^3.3.0"
  },
  "engines": {
    "node": ">=6.9.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "greenkeeper": {
    "ignore": [
      "commitizen",
      "cz-conventional-changelog",
      "conventional-changelog-cli",
      "jsarch",
      "eslint",
      "eslint-config-prettier",
      "prettier",
      "jest",
      "coveralls",
      "jsdoc-to-markdown"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "node": "6.9.5"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "jest": {
    "coverageReporters": [
      "lcov"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "contributors": []
}
